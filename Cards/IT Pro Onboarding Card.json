{
 "$schema": "https://adaptivecards.io/schemas/adaptive-card.json",
 "type": "AdaptiveCard",
 "version": "1.2",
 "body": [
  {
   "type": "ColumnSet",
   "columns": [
    {
     "type": "Column",
     "width": "stretch",
     "items": [
      {
       "type": "TextBlock",
       "text": "New on-boarding task",
       "weight": "bolder",
       "size": "large"
      },
      {
       "type": "TextBlock",
       "text": "Please create a domain account and prepare devices for the new employee.",
       "isSubtle": true,
       "wrap": true
      },
      {
       "type": "TextBlock",
       "isSubtle": true,
       "text": "Employee details:",
       "weight": "Bolder"
      }
     ]
    },
    {
     "type": "Column",
     "width": "150px",
     "items": [
      {
       "type": "Image",
       "url": "data:image/png;base64,",
       "width": "150px",
       "spacing": "small",
       "horizontalAlignment": "right",
       "height": "120px"
      }
     ]
    }
   ]
  },
  {
   "type": "ColumnSet",
   "columns": [
    {
     "type": "Column",
     "width": 2,
     "items": [
      {
       "type": "FactSet",
       "facts": [
        {
         "title": "First name",
         "value": "${FirstName}"
        },
        {
         "title": "Last name",
         "value": "${LastName}"
        },
        {
         "title": "Position",
         "value": "${Position}"
        },
        {
         "title": "Employee Id",
         "value": "${EmployeeID}"
        }
       ],
       "separator": true
      },
      {
       "type": "TextBlock",
       "isSubtle": true,
       "text": "Please fill in employee IT details:",
       "weight": "Bolder",
       "spacing": "Medium"
      },
      {
       "type": "ColumnSet",
       "columns": [
        {
         "type": "Column",
         "width": "100px",
         "items": [
          {
           "type": "TextBlock",
           "text": "Login",
           "wrap": true,
           "spacing": "None"
          }
         ]
        },
        {
         "type": "Column",
         "width": "stretch",
         "items": [
          {
           "type": "Input.Text",
           "id": "ac_userLogin",
           "placeholder": "abc123"
          }
         ]
        }
       ],
       "spacing": "None",
       "style": "emphasis"
      },
      {
       "type": "ColumnSet",
       "columns": [
        {
         "type": "Column",
         "width": "100px",
         "items": [
          {
           "type": "TextBlock",
           "text": "E-mail",
           "wrap": true,
           "height": "stretch"
          }
         ]
        },
        {
         "type": "Column",
         "width": "stretch",
         "items": [
          {
           "type": "Input.Text",
           "id": "ac_userMail",
           "placeholder": "email@example.com",
           "style": "Email"
          }
         ]
        }
       ],
       "spacing": "None",
       "style": "accent"
      },
      {
       "type": "ColumnSet",
       "columns": [
        {
         "type": "Column",
         "width": "100px",
         "items": [
          {
           "type": "TextBlock",
           "text": "Phone number"
          }
         ]
        },
        {
         "type": "Column",
         "width": "stretch",
         "items": [
          {
           "type": "Input.Text",
           "id": "ac_userPhone",
           "placeholder": "xxx.xxx.xxxx",
           "style": "Tel"
          }
         ]
        }
       ],
       "spacing": "None",
       "style": "emphasis"
      }
     ]
    }
   ]
  },
  {
   "type": "ColumnSet",
   "columns": [
    {
     "type": "Column",
     "width": "100px",
     "items": [
      {
       "type": "TextBlock",
       "text": "Computer number",
       "wrap": true
      }
     ]
    },
    {
     "type": "Column",
     "width": "stretch",
     "items": [
      {
       "type": "Input.Text",
       "placeholder": "abc-123-zbc-456",
       "id": "ac_userComputerNumber"
      }
     ]
    }
   ],
   "spacing": "None",
   "style": "accent"
  }
 ],
 "actions": [
  {
   "type": "Action.Submit",
   "title": "Confirm"
  }
 ]
}